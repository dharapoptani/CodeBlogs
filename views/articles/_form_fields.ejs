<div class="form-group mt-1">
    <label for="title">Title</label>
    <input required type="text" class="form-control mt-1" id="title" name="title" value="<%= article.title%>">
</div>
<div class="form-group mt-1">
    <label for="contibutor">Contibutor</label>
    <input required type="text" class="form-control mt-1" id="contibutor" name="contributor"
        value="<%= article.contributor%>">
</div>
<div class="form-group mt-1">
    <label for="description">Description</label>
    <textarea required rows="5" type="text" class="form-control mt-1" id="description" name="description"
       ><%= article.description%></textarea>
</div>

<div class="form-group mt-1">
    <label for="code">Code</label>
    <textarea  required type="text" class="form-control mt-1" id="code" rows="5" name="code"
        ><%= article.code%></textarea>
</div>



<a href="/" class="btn btn-secondary mt-2">Cancel</a>
<button type="submit" class="btn btn-primary mt-2">Save</button>